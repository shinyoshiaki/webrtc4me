{
  "name": "rx.mini",
  "version": "0.1.0",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "type": "tsc --noEmit",
    "update": "rm -rf ./lib && npm run build",
    "test": "npm run type && jest",
    "deploy": "npm run test && npm run update",
    "publish": "npm publish --access=public"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^11.13.6",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "tslib": "^1.9.3",
    "typescript": "^3.4.4"
  },
  "dependencies": {},
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*test.ts"
    ]
  }
}
